<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="Machiel de Jager">
        <createTable tableName="Backoffice_File">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="emailNotification_id" type="int" />
            <column name="fileSpecs_id" type="int"/>
        </createTable>
        <createTable tableName="Customer_File">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>

        </createTable>
        <createTable tableName="Email_Notification">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="gftKanaal_id" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="fileSpecs_id" type="int"/>
        </createTable>
        <createTable tableName="File_Lock">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="fileSpecs_id" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="lockerId" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="lockMOment" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="lockerProces" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <createTable tableName="File_Specs">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="fileName" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="filePath" type="varchar(1014)">
                <constraints nullable="false"/>
            </column>
            <column name="fileSize" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="fileLock_id" type="int">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <createTable tableName="File_Status">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="status" type="varchar(30)">
                <constraints nullable="false"/>
            </column>
            <column name="moment" type="int">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <createTable tableName="GFT_Kanaal">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="kanaalNaam" type="varchar(50)">
                <constraints nullable="false"/>
            </column>

        </createTable>
    </changeSet>

</databaseChangeLog>